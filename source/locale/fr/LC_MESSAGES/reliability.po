#: ../../reliability.rst:49
msgid "Protocol"
msgstr ""

# 50fdab6c4d254b51be652286846ebac8
#: ../../reliability.rst:49
msgid "Persistence Required?"
msgstr ""

# f5fcf842916f48bba5a1ef7630fe9998
#: ../../reliability.rst:51
msgid "80 or 443"
msgstr ""

